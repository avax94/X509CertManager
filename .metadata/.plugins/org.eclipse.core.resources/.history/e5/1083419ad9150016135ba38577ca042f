package sm130075.vl130298.crypto;

import java.security.PrivateKey;

public class KeyStorage {
	private PrivateKey privateKey;
	private Certificate cert;
	
	public PrivateKey getPrivateKey() {
		return privateKey;
	}
	
	public void setPrivateKey(PrivateKey privateKey) {
		this.privateKey = privateKey;
	}
	
	public Certificate getUnsignedCert() {
		return cert;
	}
	
	public void setUnsignedCert(UnsignedCert unsignedCert) {
		this.unsignedCert = unsignedCert;
	}
	
	public KeyStorage(PrivateKey privateKey, UnsignedCert unsignedCert) {
		this.privateKey = privateKey;
		this.unsignedCert = unsignedCert;
	}
}
